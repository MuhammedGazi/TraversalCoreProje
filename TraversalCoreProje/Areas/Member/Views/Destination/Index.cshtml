@using EntityLayer.Concrete
@model List<Destination>
@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/Shared/_UserLayout.cshtml";
}

<table class="table table-bordered">
	<tr>
	<th>#</th>
	<th>şehir</th>
	<th>süre</th>
	<th>kapasite</th>
	<th>fiyat</th>
	<th>detay</th>
	</tr>
	@foreach(var item in Model){
		<tr>
			<td>@item.DestinationID</td>
			<td>@item.City</td>
			<td>@item.DayNight</td>
			<td>@item.Capacity</td>
			<td>@item.Price</td>
			<td><a href="#" class="btn btn-outline-info">Detaylar</a></td>
		</tr>
	}
</table>


